#include <stdio.h>
int main(){
int n,k=0,k1;
printf("Enter the number of elements :");
scanf("%d",&n);
int s[n];
printf("Enter the elements : ");
for(int i=0;i<n;i++){scanf("%d",&s[i]);
}
printf("Enter the search key : ");
scanf("%d",&k1);

for(int i=0;i<n;i++)
{
if(s[i]==k1)
{
printf("The search element found %d\n",s[i]);
k+=k1;
}

}

if(k==0)
{
printf("Search element are not present \n");
}
else
{
printf("End of the program \n");

}
return 0;
}
