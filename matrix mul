#include <stdio.h>
int main(){
int a[2][2],b[2][2],mul[2][2],i,j,k;
printf("Enter the 2X2 matrix : ");
for( i=0;i<2;i++){
for(j=0;j<2;j++){
scanf("%d",&a[i][j]);
}}
printf("Enter the second 2X2 matrix :");
for(i=0;i<2;i++){
for(j=0;j<2;j++){
scanf("%d",&b[i][j]);
}}
for(i=0;i<2;i++){
for(j=0;j<2;j++){
mul[i][j]=0;
for(k=0;k<2;k++){
mul[i][j]=a[i][k]*b[k][j];}}}
printf("The matrix multiplication is : ");
for( i=0;i<2;i++){
for(j=0;j<2;j++){
printf("%d\t",mul[i][j]);}
printf("\n");
}
return 0;
}
